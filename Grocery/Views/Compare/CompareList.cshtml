@model IEnumerable<Online_Grocey.Models.Compare>

@{
    ViewData["Title"] = "CompareList";
}

<h1>CompareList</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CompareID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CompareID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CompareID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
